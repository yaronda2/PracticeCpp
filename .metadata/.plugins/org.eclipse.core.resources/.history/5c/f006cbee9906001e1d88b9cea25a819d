#include <iostream>
#include <list>

#ifndef SIMPLESTUCK_H_
#define SIMPLESTUCK_H_

using namespace std;

namespace hrd24
{

template<class T>
class SimpleStuck {

public:
	explicit SimpleStuck();
	virtual ~SimpleStuck();
	void Push(T name);
	void Pop();
	int Peek();
	bool IsEmpty();
	int size();

private:

	std::list<T> storage;

};

}
#endif /* SIMPLESTUCK_H_ */
